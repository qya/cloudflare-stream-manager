{
  "name": "cloudflare-stream-manager",
  "productName": "Cloudflare Stream Manager",
  "description": "A desktop application for managing Cloudflare Stream videos",
  "author": "Fais.tech",
  "version": "0.1.5",
  "private": true,
  "main": "dist-electron/main.js",
  "homepage": "https://github.com/qya/cloudflare-stream-manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/qya/cloudflare-stream-manager.git"
  },
  "license": "MIT",
  "keywords": [
    "cloudflare",
    "stream",
    "manager",
    "desktop",
    "application"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:electron": "vite build --outDir dist-electron --config vite.config.electron.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && cross-env IS_DEV=true electron .",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "start": "electron-forge start",
    "package": "npm run build && NODE_NO_WARNINGS=1 electron-forge package",
    "make": "npm run build && NODE_NO_WARNINGS=1 electron-forge make",
    "make:win": "npm run build && NODE_NO_WARNINGS=1 electron-forge make --platform=win32",
    "make:win:x64": "npm run build && NODE_NO_WARNINGS=1 electron-forge make --platform=win32 --arch=x64",
    "make:win:ia32": "npm run build && NODE_NO_WARNINGS=1 electron-forge make --platform=win32 --arch=ia32",
    "make:linux": "npm run build && NODE_NO_WARNINGS=1 electron-forge make --platform=linux",
    "make:darwin": "npm run build && NODE_NO_WARNINGS=1 electron-forge make --platform=darwin",
    "publish": "npm run build && electron-forge publish",
    "dist": "npm run make"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tus-js-client": "^3.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-wix": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-electron": "^0.15.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.cloudflare.stream-manager",
    "productName": "Cloudflare Stream Manager",
    "icon": "build/icon",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "directories": {
      "output": "dist-app"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  }
}
